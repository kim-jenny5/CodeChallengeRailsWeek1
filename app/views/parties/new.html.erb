  <%= f.datetime_field :date %>
  <%= f.label :budget %>
  <%= f.number_field :budget %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :supplies %>
  <%= f.text_field :supplies %>

  <%= f.submit %>
<%end%>


 
